		"chai-jquery": "^2.0.0",
		"cross-env": "^5.1.3",
		"css-loader": "^0.28.7",
		"extract-text-webpack-plugin": "^3.0.2",
		"gh-pages": "^1.1.0",
		"html-webpack-plugin": "^2.30.1",
		"jquery": "3.3.1",
		"jsdom": "11.6.0",
		"mocha": "5.0.0",
		"react-addons-test-utils": "15.6.2",
